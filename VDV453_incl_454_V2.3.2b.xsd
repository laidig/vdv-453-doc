			<xsd:element name="FahrtID" type="FahrtIDType"/>
			<xsd:element name="HstSeqZaehler" type="HstSeqZaehlerType"/>
			<xsd:element name="LinienID" type="LinienIDType"/>
			<xsd:element name="LinienText" type="LinienTextType"/>
			<xsd:element name="RichtungsID" type="RichtungsIDType"/>
			<xsd:element name="RichtungsText" type="RichtungsTextType"/>
			<xsd:element name="VonRichtungsText" type="RichtungsTextType" minOccurs="0"/>
