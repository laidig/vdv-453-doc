	<xsd:group name="AboGroup">
		<xsd:choice>
			<xsd:element name="AboASBRef" type="AboASBRefType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="AboASB" type="AboASBType" minOccurs="0" maxOccurs="unbounded"/>
