	<xsd:complexType name="AboAZBType">
		<xsd:sequence>
			<xsd:element name="AZBID" type="IDType"/>
			<xsd:element name="LinienID" type="LinienIDType" minOccurs="0"/>
			<xsd:element name="RichtungsID" type="RichtungsIDType" minOccurs="0"/>
