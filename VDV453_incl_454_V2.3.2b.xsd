			<xsd:element name="Linienfahrplan" type="LinienFahrplanType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="SollUmlauf" type="SollUmlaufType" minOccurs="0" maxOccurs="unbounded"/>
