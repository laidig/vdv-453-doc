			<xsd:element name="AufAZB" type="xsd:boolean" default="false" minOccurs="0"/>
			<xsd:element name="ViaHst1Lang" type="HstLangType" minOccurs="0"/>
			<xsd:element name="ViaHst2Lang" type="HstLangType" minOccurs="0"/>
			<xsd:element name="ViaHst3Lang" type="HstLangType" minOccurs="0"/>
			<xsd:element name="Via" type="ViaType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name und Prioritäten der Via-Haltestellen. Diese neue Struktur wurde nachträglich zu den drei ViaHstxLang hinzugefügt, welche weiterhin auch noch abgefüllt werden sollten um abwärtskompatibel zu bleiben.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FahrtStatus" type="FahrtStatusType"/>
			<xsd:choice>
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Aussetzer- und Durchfahrt</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="AnkunftszeitAZBPlan" type="xsd:dateTime"/>
					<xsd:element name="AnkunftszeitAZBPrognose" type="xsd:dateTime" minOccurs="0"/>
					<xsd:sequence minOccurs="0">
						<xsd:element name="AbfahrtszeitAZBPlan" type="xsd:dateTime"/>
						<xsd:element name="AbfahrtszeitAZBPrognose" type="xsd:dateTime" minOccurs="0"/>
					</xsd:sequence>
				</xsd:sequence>
				<xsd:sequence>
					<xsd:annotation>
						<xsd:documentation>Einsetzerfahrt</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="AbfahrtszeitAZBPlan" type="xsd:dateTime"/>
