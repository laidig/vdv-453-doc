			<xsd:element name="MeldungsID" type="MeldungsIDType"/>
		</xsd:sequence>
		<xsd:attribute name="Zst" type="xsd:dateTime" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ANDMeldungType" mixed="true">
		<xsd:sequence>
			<xsd:element name="KanalID" type="KanalIDType"/>
			<xsd:element name="MeldungsID" type="MeldungsIDType"/>
			<xsd:element name="FormatID" type="FormatIDType" minOccurs="0"/>
			<xsd:element name="MessageXML" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence maxOccurs="unbounded">
						<xsd:any namespace="##any" processContents="skip"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="Zst" type="xsd:dateTime" use="required"/>
		<xsd:attribute name="VerfallZst" type="xsd:dateTime" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ANDNachrichtType">
		<xsd:sequence>
			<xsd:element name="ANDMeldung" type="ANDMeldungType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="ANDMeldungLoeschen" type="ANDMeldungLoeschenType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="AboID" type="AboIDType" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="AnschlussPlanType">
