	<!--This is the actual dependency which is only to the situation exchange service-->
	<xsd:import namespace="http://www.siri.org.uk/siri" schemaLocation="siri-1.3/siri/siri_situation-v1.0.xsd"/>
	<xsd:complexType name="AbbringerFahrtLoeschenType">
