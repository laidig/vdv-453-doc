			<xsd:element name="AnkunftszeitASBPlan" type="xsd:dateTime"/>
			<xsd:element name="AnkunftszeitASBPrognose" type="xsd:dateTime"/>
			<xsd:element name="FahrtStatus" type="FahrtStatusType"/>
